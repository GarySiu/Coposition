if(<%= @updated == 'fogging' %>){
  Materialize.toast('<%= j devices_fog_message(@device) %>', 1500);
  var button = $('#fogButton<%=@device.id%>');
  button.html("<%= j fog_button_text(@device) %>");
} else {
  Materialize.toast("<%= j devices_delay_message(@device) %>", 1500);
  $('#mins').val('<%= @device.delayed %>');
}
