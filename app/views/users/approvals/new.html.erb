<br>
Enter the <%= @approvals_presenter.approvable_type %> you would like to add.
<br>
<%= form_for @approval, url: @approvals_presenter.create_approval_url do |f| %>
  <div class="row">
    <div class="col s12 input-field search">
      <%= f.text_field :approvable, @approvals_presenter.input_options %>
    </div>
  </div>
  <%= f.hidden_field :approvable_type, value: @approvals_presenter.approvable_type %>
  <%= f.button "Add", class: "waves-effect waves-light btn", type: "submit" %>
<% end %>

<script>
  var devs = <%= raw @developers.to_json %>;

  $('.search .devs_typeahead').typeahead({
    hint: true,
    highlight: true,
    minLength: 1
  },
  {
    name: 'devs',
    source: window.COPO.utility.substringMatcher(devs)
  });
</script>
