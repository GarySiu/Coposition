<div class='center-align'>
<%= link_to("Add app", new_user_approval_path(approvable_type: 'Developer'), class: "btn waves-effect waves-light") %>
</div>

<script>
$('.modal-trigger').leanModal();
Copo.Permissions.check_disabled();
Copo.Permissions.disable_access_change();
